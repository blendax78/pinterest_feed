{
  "manifest_version": 2,

  "name": "PinterestFeed",
  "description": "A Chrome Extension that will show your latest Pinterest images.",
  "version": "0.1",

  "permissions": [
    "https://secure.flickr.com/",
    "*://pinterest/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}